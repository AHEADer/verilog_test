# Nexys4 Pin Assignments
############################
# On-board Slide Switches  #
############################
set_property PACKAGE_PIN U9 [get_ports u]
set_property IOSTANDARD LVCMOS33 [get_ports u]
set_property PACKAGE_PIN U8 [get_ports y]
set_property IOSTANDARD LVCMOS33 [get_ports y]
set_property PACKAGE_PIN R7 [get_ports w]
set_property IOSTANDARD LVCMOS33 [get_ports w]
set_property PACKAGE_PIN R6 [get_ports s0]
set_property IOSTANDARD LVCMOS33 [get_ports s0]
set_property PACKAGE_PIN R5 [get_ports s1]
set_property IOSTANDARD LVCMOS33 [get_ports s1]

############################
# On-board led             #
############################
set_property PACKAGE_PIN T8 [get_ports m]
set_property IOSTANDARD LVCMOS33 [get_ports m]